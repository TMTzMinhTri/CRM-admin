{
    "env": {
        "development": {
            "presets": [
                "next/babel"
            ],
            "plugins": [
                [
                    "babel-plugin-styled-components",
                    {
                        "ssr": true,
                        "displayName": true,
                        "preprocess": false
                    }
                ]
            ]
        },
        "production": {
            "plugins": [
                [
                    "babel-plugin-styled-components",
                    {
                        "ssr": true,
                        "displayName": true,
                        "preprocess": false
                    }
                ]
            ],
            "presets": [
                "next/babel"
            ]
        }
    },
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "ssr": true,
                "minify": true,
                "transpileTemplateLiterals": true,
                "pure": true,
                "displayName": true,
                "preprocess": false
            }
        ]
    ]
}